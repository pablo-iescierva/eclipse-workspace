
public class Punto {
	private double x;
	private double y;
	
	//constructores
	
	public Punto(double x, double y) {
		super();
		this.x = x;
		this.y = y;
	}
	
	
	public Punto() {
		super();
	}


	//getters and setters
	
	public double getX() {
		return x;
	}


	public void setX(double x) {
		this.x = x;
	}


	public double getY() {
		return y;
	}


	public void setY(double y) {
		this.y = y;
	}
	
